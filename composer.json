  "require": {
    "php": ">=5.6.0",
    "composer/installers": "~1.0",
    "leafo/scssphp": "0.6.*",
    "pimple/pimple": "~3.0",
    "vanilla/htmlawed": "dev-master",
    "gridonic/princexml-php": "dev-master",
    "pressbooks/saxon-he": "dev-master",
    "pressbooks/search-regex": "1.4.16",
    "pressbooks/custom-metadata": "0.8.1-dev",
    "pressbooks/disable-comments": "1.0.0",
    "pressbooks/mce-anchor-button": "1.0.1",
    "pressbooks/mce-table-buttons": "3.2.0",
    "pressbooks/mce-superscript-subscript-buttons": "1.0.0",
    "pressbooks/pressbooks-latex": "1.0.0"
  },
  "require-dev": {
  },
  "autoload": {
  },
  "archive" : {
    "exclude": [
      ".coveralls.yml",
      ".editorconfig",
      ".gitignore",
      ".phpstorm.meta.php",
      ".travis.yml",
      "CODE_OF_CONDUCT.md",
      "CONTRIBUTING.md",
      "ISSUE_TEMPLATE.md",
      "phpunit.xml",
      "tests"
    ]
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ]
  }
}
