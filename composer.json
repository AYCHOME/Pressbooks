    {
      "type": "package",
      "package": {
        "name": "pressbooks/mce-anchor-button",
        "version": "v1.0.1",
        "dist": {
          "url": "https://github.com/pressbooks/mce-anchor-button/archive/v1.0.1.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/pressbooks/mce-anchor-button/",
          "type": "git",
          "reference": "tags/v1.0.1"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "pressbooks/mce-table-buttons",
        "version": "3.2.0",
        "dist": {
          "url": "https://github.com/pressbooks/mce-table-buttons/archive/v3.2.0.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/pressbooks/mce-table-buttons/",
